{
  "name": "ecommerce-monorepo",
  "version": "1.0.0",
  "description": "E-commerce platform monorepo",
  "scripts": {
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "customer-app:dev": "cd customer-app && npm run dev",
    "customer-app:build": "cd customer-app && npm run build",
    "admin-panel:dev": "cd admin-panel && npm run dev",
    "admin-panel:build": "cd admin-panel && npm run build",
    "rider-pwa:dev": "cd rider-pwa && npm run dev",
    "rider-pwa:build": "cd rider-pwa && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../customer-app && npm install && cd ../admin-panel && npm install && cd ../rider-pwa && npm install",
    "db:migrate": "cd backend && npm run migrate",
    "db:seed": "cd backend && npm run seed",
    "db:rollback": "cd backend && npm run migrate:rollback",
    "db:setup": "cd backend && npm run migrate && npm run seed"
  },
  "keywords": [
    "ecommerce",
    "monorepo",
    "express",
    "react",
    "vite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.5",
    "@tailwindcss/vite": "^4.1.5",
    "tailwindcss": "^4.1.5"
  }
}
